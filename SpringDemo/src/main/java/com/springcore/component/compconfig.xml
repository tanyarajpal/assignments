    	<value>Amritsar</value>
    	<value>Chandigarh</value>
    </util:list>
    
    <util:list list-class="java.util.ArrayList" id="contactList">
    	<value>987654321</value>
    	<value>923457718</value>
    	<value>923456789</value>
    </util:list>
    
    <util:list list-class="java.util.ArrayList" id="projectList">
    	<value>JavaProject</value>
    	<value>PythonProject</value>
    	<value>MERNProject</value>
    </util:list>
    
</beans>